{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "destructuring": true,
      "spread": true
    }
  },
  "rules": {
    "semi": 2,
    "import/no-unresolved": 0
  }
}
