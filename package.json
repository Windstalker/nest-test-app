{
  "name": "nest-test-app",
  "version": "1.0.0",
  "description": "Nest API Test Application",
  "repository": {
    "type": "git",
    "url": "git@github.com:Windstalker/nest-test-app.git"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "build:html": "npm run clean && mkdir dist && cp ./src/index.html ./dist/",
    "build:webpack": "webpack -p --progress --colors",
    "build": "npm run build:html && npm run build:webpack",
    "dev": "npm run build:html && webpack-dev-server --content-base ./dist/ --hot --port 4000 --history-api-fallback --inline",
    "auth_server": "node src/vendor/nestAuthServer.js"
  },
